{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Githubadf12"
		},
		"LS_gitadf_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_gitadf'"
		},
		"AzureBlobStoragefor_KEY_GIT_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretforgit"
		},
		"AzureKeyVaultingit_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultnewer.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Githubadf12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_gitadf_connectionString": {
						"value": "[parameters('LS_gitadf_connectionString')]"
					},
					"AzureKeyVaultingit_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultingit_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStoragefor_KEY_GIT_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStoragefor_KEY_GIT_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}